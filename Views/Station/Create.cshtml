@model StationName

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-12">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Station Name</a></li>
                    <li class="breadcrumb-item active">Create</li>
                </ol>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="card card-primary">
            <div class="card-header">
                <h3 class="card-title">New Station Name</h3>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-md-6">
                        <label for="StationCode">Code:</label>
                        <input type="text" class="form-control" id="StationCode" value="@Model.StationCode" placeholder="Enter Station Code">
                        <label id="ErrStationCode" style="color:red;"></label>
                    </div>
                    <div class="col-md-6">
                        <label for="StationDescription">Description:</label>
                        <input type="text" class="form-control" id="StationDescription" value="@Model.StationDescription" placeholder="Enter Station Description">
                        <label id="ErrStationDescription" style="color:red;"></label>
                    </div>
                </div>
                <span class="input-group-append">
                    <button type="button" class="btn btn-primary btn-success" id="SaveStation">Save Station</button>
                    <button type="button" class="btn btn-danger btn-flat" style="margin-left: 10px;" id="Back">Back</button>
                </span>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="~/js/StationName/Create/jquery-StationName-min.js"></script>